<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5</TargetFramework>
    <RootNamespace>Janda.Base</RootNamespace>
    <Authors>Matt Janda</Authors>
    <PackageId>GitHub.Actions</PackageId>
    <Description>This is example source code for github actions. Provides base enumeration classes.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>janda base enumeration</PackageTags>
    <RepositoryUrl>https://github.com/Jandini/GitHub.Actions</RepositoryUrl>
    <PackageProjectUrl>https://github.com/Jandini/GitHub.Actions</PackageProjectUrl>    
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>..\..\bin</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>..\..\bin</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GitVersion.MsBuild" Version="5.6.8">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>  

</Project>
